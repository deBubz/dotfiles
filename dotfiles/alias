# general use
alias xx='exit'

# git
alias ggfs='git fetch && git status'
alias ggs='git status'
alias gga='git add .'
alias ggm='git commit -m'
alias ggam='git add . && git commit -m'
alias ggs='git status'
alias ggp='git push'
alias ggget='git pull'

# firefox
alias ff-git='firefox --new-tab "https://github.com"'
alias ff-lc='firefox --new-tab "https://leetcode.com"'

